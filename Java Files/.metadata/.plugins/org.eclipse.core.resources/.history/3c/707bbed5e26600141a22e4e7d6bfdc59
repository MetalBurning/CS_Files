        int tempHashedWord = hashedWord;
        int quadraticProb = 1;
        do
        {
            if (keys[tempHashedWord] == null)
            {
                keys[tempHashedWord] = keys;
                values[tempHashedWord] = newWord;
                currentSize++;
                return;
